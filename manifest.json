{
  "manifest_version": 2,
  "name": "UnixCoin Auto",
  "description":"Extension support auto buy UNix Coin",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-1.12.4.min.js","content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}